		<script type="text/javascript">
			// enable infinite scroll
			$(document).ready(function() {

				$('section#blog').infinitescroll({
					navSelector  : "p#pagination",
					nextSelector : "p#pagination a#next",
					itemSelector : "#blog .post",
					loading: {	img : "http://static.tumblr.com/fjltjbq/wgUm4m75u/loading.gif",
								msgText: "",
								finishedMsg: "" }
					},
					function( newElements ) {

						var $newElems = $( newElements ).css({ opacity: 0 });

						var $container = $('body.index-page #blog');

					    var $newElemsIDs = $newElems.map(function () {
					        return this.id;
					    }).get();

						$newElems.imagesLoaded(function(){

							$newElems.animate({ opacity: 1 });
							$container.masonry( 'appended', $newElems, true );

							// enable likes
							Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
							$("a#like").css("line-height" , "12px");

							// re-enable fancybox and indent code
							noIndent();

						});
					}
				);
			});
		</script>
		

		


