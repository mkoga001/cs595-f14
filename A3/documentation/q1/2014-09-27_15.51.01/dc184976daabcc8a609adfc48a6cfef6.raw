	jQuery(document).ready(function(){
		// MegaMenu //	
		jQuery('#mainMenu ul li').mouseenter(function () {
			jQuery(this).children('.megaMenu').show();	
			jQuery(this).children('a:first-child').addClass('mouseOver');
		});
		
		jQuery('#mainMenu ul li').mouseleave(function () {
			jQuery(this).children('.megaMenu').hide();	
			jQuery(this).children('a:first-child').removeClass('mouseOver');
		});
		
		jQuery('#mainMenu ul li ul').parent('li').children('a:first-child').addClass('expandable');
		
		jQuery('#mainMenu ul li a.expandable').append('<span></span>');
		
		jQuery('.megaMenu ul li').mouseenter(function () {
			jQuery(this).children('ul').show();	
		});
		
		jQuery('.megaMenu ul li').mouseleave(function () {
			jQuery(this).children('ul').hide();	
		});
		
		jQuery('#mainMenu ul li:last-child').addClass('last');
	});
</script>   
