<script language="Javascript" type="text/javascript">
    function showwindow() {
        //Get url Path
        var urlpath = document.location.pathname;
        //Get complete url
        var url = location.href;
        var urlsplit = url.split(urlpath, 2);

        //Get query parameter
        var customurl = urlsplit[1];

        //Replace "&" in query string parameter with "~"
        //var customurl = customurl.replace("&", "~");
        //customurl = ApplicationPath + customurl;
        customurl = urlpath + customurl;

        var userLogginOrNot = document.getElementById('ctl00_ctl00_ctl00_pageContent_headerControl_lblNotLoggedIn');        
        if (userLogginOrNot != null) {
            window.open(WebHostName + '/CountrySelection/CountrySelection.aspx?url=' + customurl, 'CountrySelection', 'left=100,top=100,width=700,height=500,toolbar=0,resizable=0');
        }
        else {
            window.open(WebHostName + '/CountrySelection/CountryCustomerCare.aspx?url=' + customurl, 'CountrySelection', 'left=100,top=100,width=700,height=500,toolbar=0,resizable=0');
        }
        return false;
    }
	</script>
	

