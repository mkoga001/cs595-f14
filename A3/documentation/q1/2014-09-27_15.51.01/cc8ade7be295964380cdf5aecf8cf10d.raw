<script id="template-wwe_interaction-item" type="text/x-jsrender" style="display: none">
<div class="inner">
  <img src="{{>actor.avatar}}" class="avatar" />
  <div class="right">
    <div class="title"><a href="{{>actor.id}}" target="_blank">{{>actor.title}} <span>{{>actor.screenName}}</span></a></div>
    <div class="content">{{:object.content}}</div>
    <div class="likes">
      {{if likes.length>0}}
      <span class="by">Liked By:</span>
      {{for likes}}
        <span class="like"><img src="{{>actor.avatar}}" /> {{>actor.title}}</span>
      {{/for}}
      {{/if}}
    </div>
    <div class="provider clearfix">
      <img src="{{>source.icon}}" class="icon" />
      <span class="time auto">{{>timeago}}</span>
      {{if type=='twitter'}}
      <a href="https://twitter.com/intent/tweet?in_reply_to={{>source.id}}" class="reply"><span></span>Reply</a>
      <a href="https://twitter.com/intent/retweet?tweet_id={{>source.id}}" class="retweet"><span></span>Retweet</a>
      <a href="https://twitter.com/intent/favorite?tweet_id={{>source.id}}" class="favorite"><span></span>Favorite</a>
      {{else if type=='facebook'}}
      {{/if}}
    </div>
  </div>
</div>
<div class="children" id="{{>uniqueid}}-children"></div>
</script></body>
