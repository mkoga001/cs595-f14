<script type="text/javascript">
$(function () {
  
	var prevContainer = (club==="mlb") ? "#homepage_container" : "#home_container";
	
	var LiveLookIn = bam.widget.LiveLookIn;

	LiveLookIn.init({
		grep: function (medium) {
			return medium.away_team_id=="119" || medium.home_team_id=="119";
		}
	});
	LiveLookIn.zPlayer.shouldShowRelatedContent = function(){ return false; }; // disable related content for LLI	
	LiveLookIn.zPlayer.bind("startPlayback", function(){
		bam.clubhome.playerObj.execute("pausePlayback");
	});
	LiveLookIn.start();
	LiveLookIn
		.onSuccess(function(e, media){
			if ($("#drawer_container.lookin").length===0){
				var drawerHtml = '' + 
						'<div id="drawer_container" class="presizing lookin">' +
							'<a href="#" class="liveLookIn" data-calendar-event-id="'+media[0].calendar_event_id+'" data-link-origin="LA Homepage Drawer">' +
								'<strong>LIVE LOOK-IN:</strong> '+media[0].headline+' &nbsp;&nbsp; Watch Now &raquo;' +
							'</a>' +
						'</div>';
				$(drawerHtml).insertBefore(prevContainer).animate({height:"20px"}).find("a");
			}
		})
		.onEmpty(function(){
			$("#drawer_container.lookin")
			  .animate({height:"0"}, function(){ $(this).remove() });
		});
		
	// may need to use something like: eventProxy(ScoreboardTickerApp.displayLiveLookIn, ScoreboardTickerApp)
});
</script>


