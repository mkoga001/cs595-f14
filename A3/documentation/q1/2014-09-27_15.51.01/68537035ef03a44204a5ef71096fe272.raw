                <span class="game-time">
                    <span class="time">1:25 PM</span>
                    <span class="wrapper">
                        <span class="ampm">PM</span>
                        <span class="timezone">PDT</span>
                    </span>
                </span>
            </div>
        {{/if}}

        {{#if isInGame}}
            <div class="live">LIVE</div>
        {{/if}}

        {{#if isPostGame}}
            <div class="live"><span class="date">Sep 28</span> <span class="result {{calcResult clubcode}}">{{calcResult clubcode}}</span></div>
        {{/if}}

        {{#if isPostGame}}
        {{else}}
            <span class="broadcast-logo nflcsdings">{{getBroadcastCharacterMap "FOX"}}</span>
        {{/if}}

    </script>

    <script type="text/x-handlebars" id="game-status-strip-teamwrapper-partial">
        {{#if isPreGame}}
            <div class="team-wrapper">
                <div class="next-opponent">
                    {{#isAway}}
                        <span class="at">at</span>
                    {{/isAway}}
                    <span class="logo {{getOpponentClubCode}}"></span>
                    <span class="name">{{getOpponentClubDisplayName}}</span>
                </div>
            </div>
            <div class="status-action-wrapper">
                <div class="gameactionlink"><a href="http://www.pntra.com/t/RUBKRUNHSEBKTERHREBISEtERw" target="_blank" ><span class="nflcsdings">{{getGameActionLinkCharacterMap "video"}}</span>Watch Online</a></div>
            </div>
        {{else}}
            <div class="team-wrapper away {{#if isPostGame}}{{calcResult gameSchedule.visitorTeamAbbr}}{{/if}}">
                <span class="name">{{visitorClubDisplayName}}</span>
                <span class="logo {{gameSchedule.visitorTeamAbbr}}"></span>
                <span class="score">{{score.visitorTeamScore.pointTotal}}</span>
                <span class="hasBall{{#if awayHasBall}} yes{{/if}}"></span>
            </div>
            <div class="team-wrapper home {{#if isPostGame}}{{calcResult gameSchedule.homeTeamAbbr}}{{/if}}">
                <span class="hasBall{{#if homeHasBall}} yes{{/if}}"></span>
                <span class="score">{{score.homeTeamScore.pointTotal}}</span>
                <span class="logo {{gameSchedule.homeTeamAbbr}}"></span>
                <span class="name">{{homeClubDisplayName}}</span>
            </div>
            <div class="status-action-wrapper">
                {{#if isInGame}}
                    <div class="status-wrapper">
                        <span class="game-clock">{{showTimeBasedOnPhase}} {{score.phase}}</span>
                        {{#if score.down}}
                            <span class="separator">|</span>
                            <span class="down-yardage">{{getFormattedDown score.down}} & {{score.yardsToGo}}</span>
                        {{/if}}
                        {{#isNotNull score.yardline}}
                            <span class="separator">|</span>
                            <span class="field-pos">{{score.yardline}}</span>
                        {{/isNotNull}}
                    </div>
                {{/if}}
                {{#if isPostGame}}
                    <!-- Post Game state always has video icon -->
                    <div class="gameactionlink"><a href="http://www.pntra.com/t/RUBKRUNHSEBKTERHREBISEtERw" target="_blank" ><span class="nflcsdings">V</span>Rewatch the Game</a></div>
                {{else}}
                    <div class="gameactionlink"><a href="http://www.pntra.com/t/RUBKRUNHSEBKTERHREBISEtERw" target="_blank"  ><span class="nflcsdings">{{getGameActionLinkCharacterMap "video"}}</span>Watch Online</a></div>
                {{/if}}
            </div>
        {{/if}}
    </script>

    <script type="text/x-handlebars" id="game-status-strip-gameday-partial">
