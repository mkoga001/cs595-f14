function getCurrentHash(removeQuestionMark) {
    removeQuestionMark = removeQuestionMark || true;
    var hash = window.location.hash;
    var markStop = hash.indexOf('?');
    var ret;
    if (markStop == -1 || !removeQuestionMark) {
        // remove #
        ret = hash.substring(1);
    } else {
        // remove # and after ?
        ret = hash.substring(1,markStop);
    }
    return ret;
}

