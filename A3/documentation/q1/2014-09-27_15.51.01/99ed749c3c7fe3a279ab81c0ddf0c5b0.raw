<script>/* global jstz */
/**
 * @file
 * Use jstz (http://pellepim.bitbucket.org/jstz/) to determine the client's
 * timezone and add it as a property of the Drupal.nbc hash.
 */
var nbc = nbc || {};

(function(Modernizr) {
  var tz = null;

  if (Modernizr.sessionstorage) {
    tz = sessionStorage.getItem('nbc.timezone');
  }

  if (!tz) {
    var utz = jstz.determine();
    tz = utz.name();

    // Set unsupported timezones to eastern time.
    // @TODO - handle valid US timezones that aren't in the set of
    // four timezones the rest of the site uses. I'm looking at you Arizona ಠ_ಠ.
    if (tz === 'America/Phoenix') {
      tz = 'America/Denver';
    }
    else if (
      tz !== 'America/New_York' &&
      tz !== 'America/Chicago' &&
      tz !== 'America/Denver' &&
      tz !== 'America/Los_Angeles'
    ) {
      tz = 'America/New_York';
    }

    if (Modernizr.sessionstorage) {
      sessionStorage.setItem('nbc.timezone', tz);
    }
  }

  nbc.timezone = tz;

  var tzString = ' tz-' + tz.toLowerCase().replace('/', '-');
  document.documentElement.className += tzString;

}(Modernizr));

</script>
