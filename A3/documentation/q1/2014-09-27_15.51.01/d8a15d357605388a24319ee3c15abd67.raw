<script>
$(function() {
// request for the JSON file and build the playlist HTML
jQuery.getJSON('https://gdata.youtube.com/feeds/api/users/WalshAthletics/uploads?v=2&alt=json-in-script&callback=?', function(data) {
	
// attach the event handler to the playlist wrapper
$('#youtube-wrapper > .playlist').on("click", "li", function(e) {
	if (!$(this).hasClass('active')) {
		$(this).addClass('active').siblings('.active').removeClass('active');
		videoURL = $(this).find('a').attr('href');
		jwplayer().load({ file:videoURL, provider:"youtube" }).play();
	}
	return false;
});
  
var entries = data.feed.entry;
var playlistUser = data.feed.author[0].name.$t;
var output = "";
var activeURL = "";

if (entries) {
	output = "<ul>";
	
	// loop through the fetched entries and build the output
	jQuery.each(entries, function(index, entry) {
		var href = entry.link[0].href;
		var title = entry.title.$t;
		var thumbnail = entry.media$group.media$thumbnail[0].url;
		var titleLimit = 120;
			
		if (index == 0) {
			activeURL = href;
		}

		output += (index == 0) ? '<li class="active clearfix">' : '<li class="clearfix">';
			
		if (thumbnail) {
			output += '<img src="' + thumbnail + '" alt="" />';
		}
		
		title = (title.length > titleLimit) ? title.substring(0, titleLimit) : title;
		output += '<div><a href="' + href + '">' + title + '</a>';
				
		output += '</div></a></li>';
	});
    output += "</ul>";
    
    // render the playlist HTML
    $('#youtube-wrapper > .playlist > .overflow').append(output);
    
    // initiate the player with the first playlist item
jwplayer('youtube-player-holder').setup({
  flashplayer: "/info/jwplayer.flash.swf",
  html5player: "/info/jwplayer.html5.js",
  file: activeURL,
  provider: "youtube",
  width: "100%",
  aspectratio: "16:9"
});

}

});

});
