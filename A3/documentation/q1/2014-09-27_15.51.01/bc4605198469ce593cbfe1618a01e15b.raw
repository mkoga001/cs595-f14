<script type="text/javascript">
    var navigation = new MobileNavigation();

    decorateList('c-list', 'none-recursive');
    
    /* Redrawing of catalog list */
    var redrawList = function(){
        $j('ul.c-list:visible .category-title').each(function(index, element){
            var targetNode = $j(element.parentNode);
            var screenW = $j(element.parentNode.parentNode).width();
            var firstChildWidth = $j(element.parentNode.parentNode.select('.col-1')[0]).width();
            targetNode.width(screenW - firstChildWidth).height($j(element.parentNode.parentNode).height());
        });        
    };
    $j("ul.c-list li").hover(function(e){
    	$j(this).addClass('hover');},function(){$j(this).removeClass('hover');
    });
    $j(document).ready(function(){
    	redrawList();
        $j(document).bind('checkorientationchange', function(){
            redrawList();
        });
        $j('ul.c-list').bind('setHeight', function(){
        	redrawList();
        });
    });
    
</script>
