				        prepare_next_slide: function(){
				          app.move_container();
				          app.update_current_slide();
				          app.update_current_container();
				          app.populate_container();
				        },

				        move_container: function(){
				          app.vars.container = $("#slide_container" + app.vars.previous_container).detach();
				          $("#slide_container").css("left", "0px");
				          app.vars.container.appendTo("#slide_container");
				        },

				        update_current_slide: function(){
				          app.vars.current_slide++;
				          if(app.vars.current_slide == slideshow.length){
				            app.vars.current_slide = 0;
				          }
				        },

				        update_current_container: function(){
				          if(app.vars.current_container == 2){
				            app.vars.current_container = 1;
				            app.vars.previous_container = 2;
				          }else{
				            app.vars.current_container = 2;
				            app.vars.previous_container = 1;
				          }
				        },

				        populate_container: function(){
				          var slide = slideshow[app.vars.current_slide];
				          app.set_content(slide[0], slide[1], slide[2], slide[3]);
				        },

				        set_content: function(title, description, image, link){
				          var container = "#slide_container" + app.vars.current_container;
				          $(container + " > .title").html("<a href='" + link + "'>" + title + "</a>");
				          $(container + " > .dan_content").html("<a href='" + link + "'><img align='left' src='" + image + "' border='0'></a>" + description);
				        },

				        init: function(){
				          app.populate_container();
				          app.slide();
				          app.vars.slideshow_timer = setInterval("app.slide()", slide_pause_in_seconds * 1000);
				        }

				      } 

				      $(document).ready(function(){

				        $("#changable_area").mouseover(function(){
				          $("#paused").show()
				          clearInterval(app.vars.slideshow_timer);
				        });

				        $("#changable_area").mouseout(function(){
				          $("#paused").hide()
				          app.vars.slideshow_timer = setInterval("app.slide()", slide_pause_in_seconds * 1000);
				        });

				        app.init();

				      });

				    </script>
