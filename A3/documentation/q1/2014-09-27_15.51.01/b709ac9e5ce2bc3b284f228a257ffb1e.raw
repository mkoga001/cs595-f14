  <script>
    $(document).ready(function () {
      var context = '/clientrms'
      var url = '/profile/recruiting_profile/' + 2170761 +'/coach_client_activity/' +  +''
      var isContext = window.location.href.toLowerCase().indexOf(context) > -1;

      $.ajax({
        url: isContext ? context + url : url,
        type: 'GET',
        dataType: 'json',
        success: function (data) {
          if (data.applied_for_roster_opening_message.length > 0) {
            $("#roster_application").show();
          }
          if (data.interested_in_college == true) {
            $("#interested_school").show();
          }
          $("#last_activity_date").show();
          $("#coach_client_emails").html(data.coach_client_emails_message);
        }
      });
    });
