/* Commonly used Utilities */
utilsProtoype = function () {
    /* Video Player */
    this.playVideo = function (id, containerId, options) {
        if (containerId === "modal") {
            modal = true;
            containerId = "modal-video-container";

            var modal = new cllc.ui.modal();

            modal.onclose = function () {
                player.stop();
                linkTrack('overlay', 'close');
                pageTrack();
                jQuery('#' + containerId).remove();
            };
            modal.open("<div id=\"" + containerId + "\"></div>");
        }

        cllc.lazy.requires([{ src: "/crossbrand/javascript/YouTubePlayer.js" }], function () {
            player = new YouTubePlayer(id, containerId, options);
            if (options.title) {
                player.tracking.vehicle = options.title;
            }
            if (options.media_title) {
                player.tracking.title = options.media_title;
            }
            if (jQuery('#popup-gallery .loader')) {
                jQuery('#popup-gallery .loader').fadeOut(200);
                jQuery("#popup-gallery .content .pop_caption").remove();
            }
        });
    };
};

/**
* Event to show video player with customized toolbar (using Modieus Skin from Crossbrand Video Plugin).
*/
var addVideo = function ($videoSrc, width, height, $vTitle) {
    //To load crossbrand video player
    var videoContainer = '<div id="crossbrand-video-player-5" class="mod-gallery-video-container" data-type="youtube" data-video-title="' + $vTitle + '" data-player-name="vehicle_' + Cxt.year + "_dodge_" + Cxt.vehicle + '">' +
        '<div class="crossbrand-video-player-container">' +
            '<div id="crossbrand-video-player-5-player" class="crossbrand-video-player">Loading the player...</div>' +
        '</div>' +
    '</div>';
    $(".mode-gallery-full-screen-img").prepend(videoContainer);
    // check if the request coming from URL or through an event listener , based on call embed video
    if (hashFlag) {
        $(window).on(crossbrand.media.Loader.Events.ON_SUCCESS, function (evt) {
            embedVideo($videoSrc, width, height);
        });
    } else {
        embedVideo($videoSrc, width, height);
    }

};
var embedVideo = function ($videoSrc, width, height) {
    var $ele = $('#crossbrand-video-player-5');
    vidPlayer = new crossbrand.media.Video({
        width: width,
        height: height,
        container: $ele,
        src: $videoSrc,
        skin: 'modieus'
    });
};

/**
 * Remove video player and next/previous width settings.
 */
var removeVideo = function () {
    $('#crossbrand-video-player-5, #crossbrand-video-player-5-player').remove();
    if (typeof vidPlayer === 'Object') {
        // Fix for IE Issue 'Null is Null or not an Object'
        vidPlayer.player.resetInstances();
    }
};
</script>
