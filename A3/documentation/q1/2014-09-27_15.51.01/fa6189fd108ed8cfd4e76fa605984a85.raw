	color:#262626;
}
.header-clone #primary-nav .sf-menu li.active:hover {
	color:#fff;
}
#primary-nav .sf-menu li a:hover {
	color: #ef6250;
}
/* Dropdown */
#primary-nav .sf-menu ul {
	float:right;
}
#primary-nav .sf-menu ul, #primary-nav .sf-menu li {
	position:relative;
}
#primary-nav .sf-menu ul li.hover, #primary-nav .sf-menu ul li:hover a {
	position:relative;
	color:#ef6250;
	text-decoration:none;
}
#primary-nav .sf-menu li:hover li a {
	background:none;
	color:#b9b9b9;
}
/* LEVEL TWO */
#primary-nav .sf-menu ul {
	position:absolute;
	top:100%;
	left:0;
	font-size:60%;
	z-index:10000;
	height:auto;
	margin:0;
	padding:15px 0 0 0;
	background:none;
	width:auto;
}
