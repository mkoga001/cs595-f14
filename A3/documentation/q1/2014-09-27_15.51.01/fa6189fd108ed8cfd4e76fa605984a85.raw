<script type="text/javascript">
// Fix style responsive in photo and photoset page 
var pages = 3;
var current = 1;
function fix_responsive() {
	if ($("#sectionbox").hasClass("is_photo")) {
		if ($(window).width() < 1024) {
			code = $(".info").html();
			$(".info").remove();
			$("<div class='info'>" + code + "</div>").insertBefore(".likesocial");
		} else {
			code = $(".info").html();
			$(".info").remove();
			$("<div class='info'>" + code + "</div>").insertAfter(".project #images");
		}
	}
}

$(window).resize(function () {
	fix_responsive()
});

$(window).load(function () {
	fix_responsive()
});

function clear_html_posts() {
	// Delete the last ',' in subtitle of photo in home
	$(".post h3 span").each(function (i) {
		text = $(this).html().trim();
		lastChar = text.slice(-1);
		if (lastChar == ',') {
			text = text.slice(0, -1);
			$(this).html(text);
		}
	});

	// Cut and add more string title of photo in home
	$(".post h3 a").each(function (i) {

		if ($(this).text().length > 25) {
			$(this).text($(this).text().substr(0, 25));
			$(this).append('...');
		}
	});

	// If it is present a image hide the player video
	$(".post.video .imgb a img").parent().next().hide();

	// Insert the title of the post in home
	$(".post").each(function (i) {
		var title = $(this).find(".caption strong").first().text();
		if (title) {
			$(this).find(".title,.linkImage").attr("title", title);
			if (title.length > 25) title = title.substr(0, 23) + "...";
			$(this).find(".title").html(title);

		}
	});

	// Insert the image of video
	$(".post.video").each(function (i) {

		if ($(this).find(".linkImage").first().has("img").length == 0) {
			$(this).find(".caption img[alt='thumb']").first().clone().prependTo($(this).find(".linkImage").first());
		}
		/*$(this).find(".linkImage").first().css('background',"url('"+$(this).find(".caption img[alt='thumb']").first().attr('src')+"') center no-repeat");*/
	});

	// Photoset : view only first image
	$(".photoset .linkImage").each(function (i) {
		$(this).find("img").hide();
		$(this).find("img").first().show();
	});
}

// Start grid in home
function gridHome() {
	$("#grid").isotope({
		itemSelector: '.post',
		layoutMode: 'masonry'
	});
	$('#sort a').click(function (e) {
		e.preventDefault();
		var selector = $(this).text().toLowerCase().trim().replace(' ', '_');
		if (selector == 'view_all') {
			selector = '*';
		} else {
			selector = '.' + selector;
		}
		$('#sort a').removeClass('active');
		$(this).addClass('active');
		$('#grid').isotope({
			filter: selector,
			itemSelector: '.post'
		});

	});
	$("#close-project").hide();
	clear_html_posts();
}



function AjaxPagination(elem) {
	// Get the address of the link
	var href = $(elem).attr('href');
	// Getting the desired element for working with it later
	var $wrap = $('#ajax-body');
	$("#preloader").show(); // Will fade out the white DIV that covers the website.
	$("#ajax-wrap").delay(2000).hide();
	$wrap
	// Load the remote page
	.load(href + ' #ajax-content', function () {
		// Now slide it down
		$wrap.fadeIn();
		$('body').css('background-color', '#f0f0f0');
		$("#preloader").delay(250).fadeOut("slow"); // will fade out the white DIV that covers the website.
		$("body").addClass('inner');
		inner_page();
		cloneHeader();
		$('#close-project').show();
		$("#permalinkpost").show();
		$('.next,.prev').click(function () {
			AjaxPagination(this);
			return false;
		});
		$(".comments").remove();
		addThis();
		fix_responsive();
	});
}

$(document).ready(function () {
	gridHome();
	$('.post a,.nav-page').click(function () {
		AjaxPagination(this);
		return false;
	});

	/* Function to close project */
	$('#close-project').click(function (e) {
		e.preventDefault();
		// getting the desired element for working with it later
		var $wrap = $('#ajax-body');
		$("#status").fadeIn(); // will first fade out the loading animation
		$("#preloader").fadeIn("fast"); // will fade out the white DIV that covers the website.
		$('#ajax-body').html('').fadeOut();
		$('#ajax-wrap').delay(250).fadeIn();
		$("#status").fadeOut(); // will first fade out the loading animation
		$("#preloader").delay(550).fadeOut("slow"); // will fade out the white DIV that covers the website.
		setTimeout("cloneHeader()", 250);
		$("body").removeClass('inner');
		setTimeout("gridHome()", 250);
		$wrap.html('');
		$('#close-project').hide();
	});
});



$(document).ready(function(){			   
	gridHome();				
});
</script>

<script type="text/javascript">
function inner_page() {
	// Order titles and urls  in photo/photoset page
	title = $(".is_photoset .info strong,.is_photo .info strong").first().text();
	linkto = $(".info a[title='linkto']").attr('href');

	if (typeof title != 'undefined') {
		$(".is_photoset .info strong,.is_photo .info strong").first().parent().hide();
		$(".is_photoset .info ,.is_photo .info ").prepend("<h3>" + title + "</h3>");
		$(".image").attr('title', title);
		$(".image").attr('alt', title);
	}

	$(".info a[title='linkto']").parent().hide();

	if (typeof linkto != "undefined") {
		$("#linkto").attr('href', linkto);
		$("#linkto").attr('title', linkto);
		$("#linkto").html(linkto);
	} else $("#linkto").parent().css("display", "none");

	// Delete the last ',' in photo/photoset page
	$(".tags ").each(function (i) {
		text = $(this).html().trim();
		lastChar = text.slice(-1);
		if (lastChar == ',') {
			text = text.slice(0, -1);
			$(this).html(text);
		}
	});

	// Hide thumbs video and audio in internal page 
	var title = $(".caption").find("strong").first().text();
	$(".spotify_audio_player,.soundcloud_audio_player").parent().height("80px");
	$(".is_video .entry,.is_audio .entry").find(".heading2").prepend(title);
	$(".is_video .caption,.is_audio .caption").find("strong").first().hide();
	$(".is_video .caption,.is_audio .caption").find("img[alt='thumb']").first().parent().hide();
	$(".is_video .imgb iframe,.is_audio .imgb iframe").width("100%").height("100%");
	$(".is_video .tumblr_video_iframe").width("100%");
	$(".is_video .tumblr_video_iframe,.is_video .tumblr_video_iframe iframe").height("100%");
	$(".tumblr_audio_player").height("20px");
	$(".tumblr_audio_player").parent().parent().parent().height('100%');
}


</script>
