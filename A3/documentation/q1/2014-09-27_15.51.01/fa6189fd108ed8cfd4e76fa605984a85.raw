	height:90px;
	padding:49px 0 0 0;
	background:#ffffff;
}
.header-clone {
	background:#262626!important;
	display:none;
	padding:80px 0 0 0 !important;
	position:fixed !important;
	z-index:1001 !important;
	height:54px!important;
	overflow:visible
}
/* 2.1.1 Logo */
.logo {
	width:119px;
	height:24px;
