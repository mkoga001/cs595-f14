	font-family:'Lato';
	font-size:14px;
	color:#262626;
	text-transform:uppercase;
	font-weight:700;
	line-height:18px;
	float:left;
	margin:0 0 0 30px;
	padding:0 0 2px 0;
	border-bottom:solid 4px transparent;
}
#primary-nav .sf-menu li a {
	display:block;
	color: #262626;
}
