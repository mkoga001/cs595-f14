                var $container = $this.parents('.story-col-unit__load-more:first');
                $container.addClass('is-loading');
                var pageNumber = $this.data('next-page-number');
                var pageURI = '/api/feedItem/page/' + pageNumber + '/size/10/homepage';

                jQuery.ajax({
                    type: 'GET',
                    url: pageURI,
                    traditional: false
                })
                .done(function(data){

                    var $addlFeedItems = jQuery(data);
                    var nextPageNumber = parseInt($addlFeedItems.data('next-page-number'), 10);
                    $container
                        .before($addlFeedItems.html())
                        .slideDown('fast')
                        .parent().imagesLoaded().always(function() {
                            jQuery(window).trigger('reload_sticky_sidebar_ads');
                        });
                    if (nextPageNumber !== undefined && nextPageNumber !== -1) {
                        $this.data('next-page-number', nextPageNumber);
                    } else {
                        $container.hide(); // no more pages to load
                    }
                })
                .fail(function(data){
                    $container.before('<article class="story-item error"><p>Unable to load additional items at this time. Please try again shortly.</p></article>')
                })
                .always(function(){
                    $container.removeClass('is-loading');
                });
            });

        });
    </script>
    <script>
        register_responsive_ad('tabletAd', function() {
            CN.dart.call('tabletAd', {sz:'300x250', kws:["top"], collapse: true});
        });

        register_responsive_ad('rightrailTop', function() {
            CN.dart.call('rightrailTop', {sz:'300x250', kws:["top"], collapse: true});
        });

        register_responsive_ad('leftrailBottom', function() {
            CN.dart.call('leftrailBottom', {sz:'300x250', kws:["bottom"], collapse: true});
        });
    </script>

		</div>
