			var result = app.ajax.getJson({
				url: url,
				callback: function(data) {
					if (data.status == "success") {
						// Successfully set, now we want to close the window and then update the header instead of redirecting to home,
						// This can just be done via a dialog close and then a page refresh.  Or just redirect to home if this is not a 
						// dialog.
						
							
								app.dialog.close();
							
							window.location.reload();
						
					}
				}
			});
		}
	
		function createCookie( name, value, days, path, domain, secure )
		{
			var today = new Date();
			today.setTime( today.getTime() );
			
			if (days)
			{
				days = days * 1000 * 60 * 60 * 24;
			}
			var expires_date = new Date( today.getTime() + (days) );
			
			document.cookie = name + "=" +escape( value ) +
				( ( days ) ? ";expires=" + expires_date.toGMTString() : "" ) +
				( ( path ) ? ";path=" + path : "" ) +
				( ( domain ) ? ";domain=" + domain : "" ) +
				( ( secure ) ? ";secure" : "" );
		}
	</script>
	
		
