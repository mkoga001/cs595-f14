        <script> //Fb Login
            window.fbAsyncInit = function() {
                FB.init({
                    appId: '257437034415411',
                    status: true, // check login status
                    cookie: true, // enable cookies to allow the server to access the session
                    xfbml: true  // parse XFBML
                });
                FB.Event.subscribe('auth.authResponseChange', function(response) {
                    // Here we specify what we do with the response anytime this event occurs. 
                    if (response.status === 'connected') {
                        FB.api(
                                {
                                    method: 'fql.query',
                                    query: 'SELECT name, pic_square, sex, birthday, uid, email FROM user WHERE uid=' + response.authResponse.userID
                                },
                        function(response) {
                            var dataString = 'name=' + response[0].name + "&profile_pic=" + response[0].pic_square + "&sex=" + response[0].sex + "&dob=" + response[0].birthday + "&facebookID=" + response[0].uid + "&email=" + response[0].email;
                            
                            $.ajax({
                                type: "POST",
                                url: "src_php/fb_login.php",
                                data: dataString,
                                cache: false,
                                success: function(html) {
                                    window.location.href = html;
                                }
                            });
                        }
                        );
                    } else if (response.status === 'not_authorized') {
                        FB.login();
                    } else {
                        FB.login();
                    }
                });
            };
        </script>
        <script> // Login google+
            (function() {
                var po = document.createElement('script');
                po.type = 'text/javascript';
                po.async = true;
                po.src = 'https://apis.google.com/js/client.js?onload=onLoadCallback';
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(po, s);
            })();
            function onLoadCallback()
            {
                gapi.client.setApiKey('AIzaSyA_HPI-x_oYizYDtwHRfX8aTT4MNA29t5A'); //set your browser API KEY
                gapi.client.load('plus', 'v1', function() {
                });//Load Google + API
            }
            function login()
            {
                var myParams = {
                    'clientid': '614241644417-nd8tff6iip0u8geom69s8aknvcb57qcj.apps.googleusercontent.com', //You need to set client id
                    'cookiepolicy': 'single_host_origin',
                    'callback': 'loginCallback', //callback function
                    'approvalprompt': 'force',
                    'scope': 'https://www.googleapis.com/auth/plus.login https://www.googleapis.com/auth/plus.profile.emails.read'
                };
                gapi.auth.signIn(myParams);
            }
            function loginCallback(result)
            {
                if (result['status']['signed_in'])
                {

                    var request = gapi.client.plus.people.get({'userId': 'me'});

                    request.execute(function(resp)
                    {
                        var email = '';
                        if (resp['emails'])
                        {
                            for (i = 0; i < resp['emails'].length; i++)
                            {
                                if (resp['emails'][i]['type'] == 'account')
                                {
                                    email = resp['emails'][i]['value'];
                                }
                            }
                        }
                        var dataString = 'name=' + resp.displayName + "&profile_pic=" + resp.image.url + "&sex=" + resp.gender + "&dob=" + resp.birthday + "&googleID=" + resp.id + "&email=" + email;
                        $.ajax({
                            type: "POST",
                            url: "src_php/google_login.php",
                            data: dataString,
                            cache: false,
                            success: function(html) {
                                window.location.href = html;
                            }
                        });
                    });

                } else {

                }

            }
            function logout()
            {
                gapi.auth.signOut();
                location.reload();
            }
        </script>
