    <!-- [COOKIE SLIDER] -->
    <script type="text/javascript">
        function ShowSlide() {
            $j("#popUp").css("display", "block");
            SetAnotherCookie(); // Set Second Cookie            
        }

        function setSecondCookie(cname, cvalue) {
            //var d = new Date();
            //d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            
            var expires = "expires=";  // No expiration - Deletes on Exit
            document.cookie = cname + "=" + cvalue + "; " + expires;
        }

        function getSecondCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
            }
            return "";
        }

        function checkCookie() {
            var sliderval = getSecondCookie("slider");
            if (sliderval == "S1") {
                // popup=1 will always have slider out first
                if (document.referrer.indexOf('popup%3D1') > 0 || window.location.search.indexOf("popup=1") > -1) {
                    $j("#popUp").css("right", "152px");

                    // Show initially, stop, and slowly fade right
                    $j("#popUp").stop(true, true).delay(2000).animate({
                        right: "-397px"
                    }, { duration: 2000 });

                }
                else {
                    // S1 Value then display
                    $j("#popUp").css("display", "block");
                }

            }
            else {
                $j("#popUp").css("right", "152px");

                // Show initially, stop, and slowly fade right
                $j("#popUp").stop(true, true).delay(2000).animate({
                    right: "-397px"
                }, { duration: 2000 });

            }

        }

        function SetAnotherCookie() {
            setSecondCookie("slider", "S1"); // Set Value S1            
        }

        $j(document).ready(function () {
            checkCookie(); // Must load after            
        });
    
    </script>

    <!-- CHECK FORM -->
    <script language="javascript" type="text/javascript">
        var $j = jQuery.noConflict();

        function EmailCheckForm() {
            var objRegExpEmail = /^[_0-9a-zA-Z-]+(\.[_0-9a-zA-Z-]+)*@[0-9a-zA-Z-]+(\.)+([0-9a-zA-Z-]+)([\.0-9a-zA-Z-])*$/;
            var objRegExpZipCode = /[~!@\#$%^&*\()\=+_'\"<>]/gi;
            var form_pass = true;
            var err_msg = "";

            if (!$j('#inputEmailAddress').val()) {
                err_msg = "Please enter a valid e-mail address";
                form_pass = false;
            } else {
                if (!objRegExpEmail.test($j('#inputEmailAddress').val())) {
                    err_msg = "Please re-enter your e-mail address";
                    form_pass = false;
                }
            }


            if (!form_pass) {
                alert(err_msg);
            }
            else {

                var url = AppPath + "/Ajax/Subscribe.aspx?email=" + $j('#inputEmailAddress').val();
                $j.ajax({
                    async: false,
                    type: "GET",
                    url: url,
                    success: function (result) {

                        if (result == 'OK') {
                            document.getElementById("slideForm").action = '/SubScribe/SubScribe.aspx?br=f21&email=' + $j('#inputEmailAddress').val();
                            document.getElementById("slideForm").submit();
                        }

                    }
                });
            }
        }

    </script>

