<script type="text/javascript">
// We want to prevent old android browsers from using features that don't function properly on them.
(function() {
	var isAllowed = true, versionString, versionParts;
	isAllowed &= navigator.userAgent.indexOf("Silk/1") === -1; // Android Silk Browser, v1.
	if (isAllowed && navigator.userAgent.indexOf("Android") > -1) {
		// Let's try to get the Android version string.
		versionString = navigator.userAgent.match(/Android (([\d]|\.)*)/);
		if (versionString.length > 1) {
			versionString = versionString[1]; // Just the version string
			versionParts = versionString.split('.');
			if (parseInt(versionParts[0]) <= 2) {
				isAllowed = false;
				if (versionParts.length > 1 && parseInt(versionParts[1]) > 3) {
					isAllowed = true;
				}
			}
		}
	}
	if (!isAllowed) {
		jQuery("body").addClass("old-android");
	}
	if (navigator.userAgent.match(/[\W]i(pad|phone|pod)[\W]/i) !== null && navigator.userAgent.match(/[\W]OS [0-5]_/) !== null) {
		jQuery("body").addClass("old-ios");
	}
	if (navigator.userAgent.indexOf("Safari") > -1 && navigator.userAgent.indexOf("Chrome") === -1 && navigator.userAgent.indexOf("Android") === -1) {
		// We're in Safari
		jQuery("body").addClass("safari");
		// Check to see if we're in "old safari" (prior to Safari 6)
		isAllowed = false;
		versionString = navigator.userAgent.match(/Version\/(([\d]|\.)*)/);
		if (Modernizr.touch) {
			isAllowed = true;
		}
		else if (versionString.length > 1) {
			// We could just compare the first digit currently, but this is more scalable if we find versions between Safari 5 and Safari 6 to not consider old.
			versionParts = versionString[1].split('.');
			if (parseInt(versionParts[0]) >= 6) {
				isAllowed = true;
			}
		}
		if (!isAllowed) {
			jQuery("body").addClass("old-safari");
		}
	}
})();
</script>
