    <script type="text/javascript">
      $(function() {
        var day_names = ['Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday'],
            $nowPlayingName = $("#now-playing div.show-name"),
            $nowPlayingBio = $("#now-playing div.show-bio"),
            $nowPlayingTime = $("#now-playing div.show-time"),
            $upNextName = $("#up-next div.show-name"),
            $upNextBio = $("#up-next div.show-bio"),
            $upNextTime = $("#up-next div.show-time"),

            convertToJSHour = function(hour, meridian) {
              var am_match = new RegExp('AM', 'i'),
                  pm_match = new RegExp('PM', 'i');

              if (hour === 12 && meridian.match(am_match)) {
                return 0;
              } else if (hour === 12 && meridian.match(pm_match)) {
                return 12;
              } else if (meridian.match(pm_match)) {
                return (hour + 12);
              } else {
                return hour;
              }
            },

            writeNowPlaying = function(name, bio, showtime) {
              $nowPlayingName.html(name);
              $nowPlayingBio.html(bio);
              $nowPlayingTime.html(showtime);
            },

            writeUpNext = function(name, bio, showtime) {
              $upNextName.html(name);
              $upNextBio.html(bio);
              $upNextTime.html(showtime);
            },

            findShow = function(showEntries, targetDay, targetHour, writeFunction) {
              //input code here
              var row = 2,
              day_search = new RegExp(day_names[targetDay]);

              for(var i = 0; i < showEntries.length; i++) {
                // continue on if the current row in the spreadsheet is not equal to the
                // desired row
                if (showEntries[i].gs$cell.row != row) { continue; }

                // set necessary variables based on spreadsheet data
                var entry = showEntries[i],
                    showName = showEntries[i].content.$t,
                    showBio = showEntries[i+1].content.$t,
                    showDays = showEntries[i+2].content.$t,
                    showStartTime = showEntries[i+3].content.$t,
                    showEndTime = showEntries[i+4].content.$t;

                // look and see if the day matches today
                if (showDays.match(day_search) && showStartTime && showEndTime) {
                  var showStartMeridian = showStartTime.split(' ')[1],
                      showStartHour = convertToJSHour(parseInt(showStartTime.split(':')[0]), showStartMeridian),
                      showEndMeridian = showEndTime.split(' ')[1],
                      showEndHour = convertToJSHour(parseInt(showEndTime.split(':')[0]), showEndMeridian);

                  // fix hours if time bleeds into next day
                  if (showEndHour < showStartHour) {
                    if (targetHour > showEndHour) {
                      showEndHour += 24;
                    } else {
                      showStartHour -= 24;
                    }
                  }

                  if (targetHour >= showStartHour && targetHour < showEndHour) {
                    writeFunction(showName, showBio, showStartTime + " - " + showEndTime);
                    return {
                             showName: showName,
                             showBio: showBio,
                             showStartTime: showStartTime,
                             showEndTime: showEndTime,
                             showStartHour: showStartHour,
                             showEndHour: showEndHour
                           };
                  }
                }

                // increment counters
                i += 4;
                row++;
              }

              writeFunction("Stay tuned! Radio the way it should be heard.", '', '');
              return null;
            },

            parseGoogleFeed = function(spreadsheetdata) {
              var entries = spreadsheetdata.feed.entry,
              now = new Date();

              // find now playing
              nowPlaying = findShow(entries, now.getDay(), now.getHours(), writeNowPlaying);

              // find what's next
              if (nowPlaying) {
                var nextDay = now.getDay();
                if (nowPlaying.showEndHour >= 24) {
                  nowPlaying.showEndHour -= 24;
                }
                if (nowPlaying.showEndHour < nowPlaying.showStartHour) {
                  nextDay += 1;
                }
                upNext = findShow(entries, nextDay, nowPlaying.showEndHour, writeUpNext);
              }
            },

            fetchGoogleData = function() {
              var url = "https://spreadsheets.google.com/feeds/cells/0AoVYX2wQ-cShdGlsODNZdXdSa2kzLUtIQ3lxZlBYWHc/od6/public/values?alt=json-in-script&callback=?";
              $.getJSON(url, {}, parseGoogleFeed);
            };

        setInterval(fetchGoogleData, 60000);
        fetchGoogleData();
      });
    </script>   
