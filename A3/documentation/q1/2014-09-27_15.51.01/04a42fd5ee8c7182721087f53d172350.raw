                                <% var completed = 100 - ((series.episode.duration_seconds - series.episode.watched_seconds) / series.episode.duration_seconds) * 100,
                                         percent = series.episode.watched_seconds > series.episode.duration_seconds ? 100 : completed; %>
                                    <div class="bar" style="width: <%=percent%>%;"></div>
                                </div>
                            </div>
                            <img alt="<%=series.title%>" onerror="this.src='//www.dramafever.com/static/11b3d60/v2/img/fallbacks/popular-placeholder.jpg'" src="<%=series.thumbnail%>">
                        </div>
                        <div class="details">
                            <div class="title"><a href="<%=series.url%>"><%=series.title%></a></div>
                        </div>
                        <div class="clearfix"></div>
                    </li>

                <% }); %>

                <% _.each(recommended, function(series){ %>
                    <li class="col-md-2 col-sm-2 recommend">
                            <% if (boomtrain) { %>
                            <div class="series-thumb pull-left watching" data-url="<%=series.www_url%>" style="width:100%" onclick="_gaq.push(['_trackEvent','DramaRecTest', 'Click', '<%= series.id %>']);" >
                            <div class="series-overlay lastwatched-overlay hidden-xs" onclick="_gaq.push(['_trackEvent','DramaRecTest', 'Click', '<%= series.id %>']);" >
                                <div class="details pull-left hidden-sm">
                                    <h6><a href="<%=series.www_url%>" onclick="_gaq.push(['_trackEvent','DramaRecTest', 'Click', '<%= series.id %>']);"><%=series.name%></a></h6>
                                </div>
                                <div class="action-bar">
                                        <a class="btn btn-action" href="<%=series.www_next_url%>" onclick="_gaq.push(['_trackEvent','DramaRecTest', 'Click', '<%= series.id %>']);">Watch</a>
                                    <div class="clearfix"></div>
                                </div>
                            <% } else { %>
                            <div class="series-thumb pull-left watching" data-url="<%=series.www_url%>" style="width:100%" onclick="_gaq.push(['_trackEvent','DramaRecControl', 'Click', '<%= series.id %>']);" >
                            <div class="series-overlay lastwatched-overlay hidden-xs" onclick="_gaq.push(['_trackEvent','DramaRecControl', 'Click', '<%= series.id %>']);">
                                <div class="details pull-left">
                                    <h6><a href="<%=series.www_url%>" onclick="_gaq.push(['_trackEvent','DramaRecControl', 'Click', '<%= series.id %>']);"><%=series.name%></a></h6>
                                </div>
                                <div class="action-bar">
                                        <a class="btn btn-action" href="<%=series.www_next_url%>" onclick="_gaq.push(['_trackEvent','DramaRecControl', 'Click', '<%= series.id %>']);">Watch</a>
                                    <div class="clearfix"></div>
                                </div>
                            <% } %>
                            </div>
                            <img alt="<%=series.name%>" onerror="this.src='//www.dramafever.com/static/11b3d60/v2/img/fallbacks/popular-placeholder.jpg'" src="<%=series.slider%>">
                        </div>
                        <div class="details">
                            <div class="title">Recommended for you</div>
                        </div>
                        <div class="clearfix"></div>
                    </li>

                <% }); %>
        </script>

        <script id='fb-friend-template' type="text/template">
          <li class="col-xs-12 col-md-4">
            <div class="friend-wrap">
                <img class='fb-profile-img' src="<%= friend.pic_square %>"></img>
                <% if (friend.name.length < 25) { %>
                    <h4><%= friend.name %></h4><br/>
                <% } else { %>
                    <h4><%= friend.name.slice(0, 21) + "..." %></h4><br/>
                <% } %>
                <button class="fb-invite invite" data-fb-uid="<%= friend.uid %>">
                  <span class="invite"><i class="icon-ok"></i> Invite to DramaFever</span>
                  <span class="invited"><i class="icon-ok"></i> Invited!</span>
                </button>
                <div class="clearfix col-xs-12"></div>
            </div>
          </li>
        </script>


        <script id='google-friend-template' type="text/template">
          <li class="col-xs-12 col-md-4">
            <div class="friend-wrap">
                <img class='fb-profile-img' src="<%= friend.image.url %>"></img>
                <% if (friend.displayName.length < 25) { %>
                    <h4><%= friend.displayName %></h4><br/>
                <% } else { %>
                    <h4><%= friend.displayName.slice(0, 21) + "..." %></h4><br/>
                <% } %>
                <button
                  class="g-interactivepost invite"
                  data-contenturl="http://www.dramafever.com/"
                  data-clientid= "111011554853-bb6p7b01e8mp2veb9onhk54bmvog1trr.apps.googleusercontent.com"
                  data-cookiepolicy="none"
                  data-prefilltext="Sign up for DramaFever and watch your favorite dramas in HD without ads for free!"
                  data-calltoactionlabel="DISCOVER"
                  data-calltoactionurl="http://www.dramafever.com/"
                  data-recipients="<%=friend.id%>"
                  onclick="$(this).removeClass('invite'); $(this).addClass('invited')">
                    <span class="invite"><i class="icon-ok"></i> Invite to DramaFever</span>
                    <span class="invited"><i class="icon-ok"></i> Invited!</span>
                </button>
            </div>
          </li>
        </script>


            
            <script src="//cdn.optimizely.com/js/1464009.js"></script>

        </body>
