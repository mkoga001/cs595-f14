        <script>
var dataLayer = [];

$(document).ready(function() {
	$.dramafever.user.done(function(user) {
		// $.dramafever.log(['pushing user to dataLayer', user]);
		var data = {
			'username': user ? user.username : null,
			'language': $.dramafever.default_lang,
			'userType': $.dramafever.userType(),
		};

		if (user !== null && user.premium_level !== null) {
			data['premium_level'] = user.premium_level;
		}

		window.dataLayer.push(data);

		window.dataLayer.push({
			'event':'userdone',
			'category': 'user',
			'action': 'done',
			'label': data.userType,
			'value' : data.premium_level
		});
	});

	// if additional segment data comes in, fire it separately
	$.dramafever.segments.done(function(segments, x) {
		// $.dramafever.log(['pushing segments to dataLayer', segments]);
		window.dataLayer.push({
			'country': segments.c,
			'e': segments.e,
			'l': segments.l,
			'a': segments.a,
		});
	});
});
</script>

