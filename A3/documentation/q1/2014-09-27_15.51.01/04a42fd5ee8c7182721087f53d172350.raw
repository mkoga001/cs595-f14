        <script type="text/javascript">
        $(document).ready(function(){

            $.dramafever.static_url = '//www.dramafever.com/static/11b3d60/';
            $.dramafever.media_url = '//www.dramafever.com/st/';

            $.dramafever.is_authenticated=false;

            $.dramafever.authurl="https://www.dramafever.com/auth/xln/";

            $('.ratings').each(function() {
                $(this).starRatings();
            });

            $(document).on('click', '.joinfb, .joinfb-large', function() {

                var refresh = $(this).hasClass('fb-refresh');

                FB.login(function(response) {
                    if (response.authResponse) {
                        if(refresh) {
                            location.reload();
                        } else if (window.location.pathname === '/signup/') {
                            if(window.location.search.indexOf("?pl=") == 0){ // with purchase intention, "pl" stands for premium_level
                                window.location = 'https://www.dramafever.com/signup/premium/create/';
                            }else{
                                window.location = 'https://www.dramafever.com/signup/premium/?sk=1';
                            }
                        }
                    }
                }, {scope: 'email, publish_actions, user_birthday'});
            });

            $('div.series-thumb-container').find('img').error(function() {
                $(this).attr('src', '//www.dramafever.com/static/11b3d60/img/default-header-placeholder.jpg');
            });


            $("#forgot-pass-btn").click(function(e) {
                e.preventDefault();
                var data = "reg_email=" + encodeURIComponent($("#reg_email").val());

                $.ajax({url:"/forgot/",
                    type:"POST", data:data, dataType:"html", cache:false,
                    success:function(data){ $("#forgot-resp").html(data); $("#reg_email").val("");},
                    error:function(xhr, status, err){  }
                });
                return false;
            });

        });
        </script>

        


        
        
