			}
			return value;
		},
		_getDivId: function(value){
			if(!value){
				value = this.get('contentBox').get('id');
			}
			return value;
		},
		_getCorrelator: function(){
			return Constants.AD_RANDOM;
		},
		_getAdSlot: function(){
			return this._adSlot;
		},
		_getPath: function(){
			var slotName = this.get('slotName'),
				targeting = this.get('targeting'),
				sizes = this.get('sizes'),
				size = (sizes && sizes[0]) ? sizes[0] : [this.get('width'), this.get('height')],
				qs = {
					sz: size.join('x'),
					iu: slotName,
					tile: this.get('tile'),
					t: encodeURIComponent(Y.QueryString.stringify(targeting)),
