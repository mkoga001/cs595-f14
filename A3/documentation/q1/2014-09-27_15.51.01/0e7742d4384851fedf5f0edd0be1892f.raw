					return Y.Cookie.exists(COOKIE_AAMNFL) ? (this.get('isLegacy') ? Y.Cookie.get(COOKIE_AAMNFL) : encodeURIComponent(Y.Cookie.get(COOKIE_AAMNFL).replace(/;/g, '&'))) : '';
				}
			},
