        <script>
    var add_to_cart_pending = false,
    custom_ww_add_to_cart_handler = function(s) {
        try {
            var totalValue = 0;
            for (var i = 0; i < 20; ++i) {
                var qty = $('#id_form-' + i + '-quantity'),
                    price = 0,
                    fee_total = 0,
                    faceV1 = $('#id_form-' + i + '-price'),
                    feesV1 = $('#id_form-' + i + '-fees'),
                    nyopt = $('input[name="form-' + i +'-variable_price"]:visible'),
                    faceV2 = qty.closest('tr').find('.ticket-type-price').data('price'),
                    feesV2 = qty.closest('tr').find('.ticket-type-fees').data('fees'),
                    face = faceV1.length ? parseFloat(faceV1.val()) : faceV2,
                    fees = feesV1.length ? parseFloat(feesV1.val()) : feesV2;

                if (qty.length && qty.val() > 0) {
                    if (nyopt.length) {
                        price = parseFloat(nyopt.val());
                    } else if (face) {
                        price = face;
                    } else {
                        price = 0.0;
                    }

                    if (fees) {
                        fee_total = fees;
                    } else {
                        fee_total = 0.0;
                    }

                    totalValue += parseInt(qty.val()) * (price + fee_total);
                }
            }
            totalValue = Math.round(totalValue*100)/100;
            custom_ww_send_to_star('add_to_cart', totalValue);
        } catch (e) {
            console && console.log(e);
        }
    },
    custom_ww_configure_star = function(oncomplete) {
        window.smpAsyncInit = function() {
            StarSMP.smpLogin = function (cb) {cb({});};
            StarSMP.init(
                {
                    //loyaltyId: '184365', /* TEST */
                    loyaltyId: '135075', /* PROD */
                    apiServer: '//api.starsmp.com/',
                    clientId: 'world',
                    channelUrl: '//' + document.location.host + '/channel.html',
                    smpLoginService: false
                },
                oncomplete || function() {}
            );
        };

        (function(d, s, id){
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) {return;}
            js = d.createElement(s); js.id = id;
            js.src = '//api.starsmp.com/connect/clients/world.js';
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'starsmp-jssdk'));
    },
    set_tl_star_cookie = function (name, value) {
        document.cookie = name+"="+escape(value)+"; path=/";
    },
    custom_ww_send_to_star = function(label, amount) {
        try {
            StarSMP.track(label, document.location.href, undefined, amount);
            console && console.log('Sent ' + amount + ' to ' + label);
        } catch(e) {
            console && console.log(e);
        }
    }
    wrapped_add_to_cart = function(e) {
        if (add_to_cart_pending) { return false; }
        add_to_cart_pending = true;
        custom_ww_add_to_cart_handler(); 
        setTimeout(function() { add_to_cart_pending = false; $('.tickets-form').submit(); }, 1000);  // Leave some time for star call to complete
        return false;
    };

    window.onload = function(){
        $(function(){
            try {
                custom_ww_configure_star(
                    function() {
                        set_tl_star_cookie('ticketleap_star_smp_access_token', StarSMP.accessToken);
                        console && console.log('Cookie', StarSMP.accessToken);
                    }
                );
                var pageId = $('meta[name="tl:id"]').attr('content');
                var uiIsV2 = $('.hero-container').length > 0;
                if (/^ticketleap|event|/i.test(pageId) && $('.add-tt-to-cart, #order-summary .checkout').length) {
                    $('body').on(uiIsV2 ? 'click' : 'mousedown', '.add-tt-to-cart:not(.tl-buttons-disabled)', function(e) {
                        if (uiIsV2) { return wrapped_add_to_cart(e); }
                        else { custom_ww_add_to_cart_handler(); return true; }
                    });
                    $('body').on(uiIsV2 ? 'click' : 'mousedown', '#order-summary .checkout:not(.tl-buttons-disabled)', function(e) {
                        if (uiIsV2) { return wrapped_add_to_cart(e); }
                        else { custom_ww_add_to_cart_handler(); return true; }
                    });
                }
            } catch (e) {
                console && console.log(e);
            }
        });
    };
</script>

