		$('#embed .selectcustom input').ColorPicker({
			onSubmit: function(hsb, hex, rgb, el) {
				$(el).val(hex);
				$(el).ColorPickerHide();
				fetch_embed();
			},
			onBeforeShow: function() {
				$(this).ColorPickerSetColor(this.value);
			}
		}).bind('keyup', function() {
			$(this).ColorPickerSetColor(this.value);
		}).bind('change', function() {
			$(this).ColorPickerHide();
			fetch_embed();
		});

		$("#itsembed").click(fetch_embed);

		$("#save-custom-colors").click(function(e) {
			var params = {};
			$("#embed .selectcustom input").each(function() {
				params[this.name] = this.value;
			});

			$.ajax({
				url: '/embed3/save-custom-colors',
				cache: false,
				data: params,
				type: 'POST'
			});
		});
	});

	function fetch_embed() {
		var params = {demo: true};
		$("#embed .selectcustom input").each(function() {
			params[this.name] = this.value;
		});

		var src = "av-1/success?";
		var dmn = "www.audiomack.com";

		$("#embed .inembed").empty().append($("<iframe scrolling='no' width='100%' height='352' scrollbars='no' frameborder='0' src='/embed3-album/"+src+$.param(params)+"'></iframe>"));

		delete params['demo'];
		$("#embed .embed-text").val("<iframe src='"+document.location.protocol+"//"+dmn+"/embed3-album/"+src+$.param(params)+"' scrolling='no' width='100%' height='352' scrollbars='no' frameborder='0'></iframe>");
	}
</script>

