        	if(this.options.filterBy){// If we are filtering out specific types of media
        		var _filter = this.options.filterBy;
				var attachment;
        		for( i=0; i<content.attachments.length; i++){
					attachment = content.attachments[i];
        			//What kind of media is it link, photo, rich (video/iframe)
        			if(_filter.type && _filter.type != '' && 
        			   _filter.type.toLowerCase().indexOf(attachment.type.toLowerCase()) == -1 ){
        				return done();// If the type isn't approved skip this content
        			}
        			//What type of media is it? (Instagram, Vine, Spotify)
        			if(_filter.provider_name && _filter.provider_name != '' && 
        			   _filter.provider_name.toLowerCase().indexOf(attachment.provider_name.toLowerCase()) == -1 ){
        				return done();
        			}
        			//Where did it come from (Twitter, Facebook, etc)
        			if(_filter.source && _filter.source != '' && 
        			   _filter.source.toLowerCase().indexOf(content.source.toLowerCase()) == -1 ){
        				return done();
        			}
        		}
        	}
        	
            this.push(content);
            return done();
        }
        content.on('attachment', function () {
            self.push(content);
        });
        done();
    };
    
    /**
     * A Livefyre Media wall
     */
	function MediaWall(_el, settings){
	
		var defaults = {
			config:{"network": "umg.fyre.co"},
			initial: 30, more:30, showMore:true
	    };
		this.options = $.extend(true,{}, defaults, settings ); 
		//TODO allow conditional different Views.
		this.view = new WallView({//create the media wall display
	        initial: this.options.initial,
	        showMore: this.options.more,
	        el:_el
	    });
		
		this.collection = new Collection(this.options.config); //setup the collection data
		
		this.archive = this.collection.createArchive();
		this.updater = this.collection.createUpdater();
		
		if(this.options.filterBy){//Setup media filtering
			var _media = new MediaFilter(this.options);
			this.archive = this.archive.pipe(_media);
			this.updater = this.updater.pipe(_media);
		}
		
		if(this.options.showMore){//Setup show more button
			this.archive.pipe(this.view.more);
		}
		
		this.updater.pipe(this.view);
		
		return this;
	}
	
	new MediaWall(document.getElementById('lf-media-wall'), { 
		config:{ siteId: 336025, articleId: 'carlyraejepsen_featured' }
	});
}(jQuery));
