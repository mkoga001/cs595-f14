  {
  	if(s==null || s==undefined)
  	{
  		return true;
  	}
  	var i = 0;
  	for(i=0;i<s.length;i++)
  	{
  		if((s.charAt(i)!=undefined) && (s.charAt(i)!=null) && (s.charAt(i)!=' ') && (s.charAt(i)!=''))
  		{
  			return false;
  		}
  	}
  	return true;
  }
  
  
  function validateForm(form)
  {
  	//if(!frmChecked)
