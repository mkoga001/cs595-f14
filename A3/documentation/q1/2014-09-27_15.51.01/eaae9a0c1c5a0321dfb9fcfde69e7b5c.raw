  function checkRequiredFields(form)
  {
  	if(isEmpty(form.firstName_bill.value) || (form.firstName_bill.value == "First Name"))
  	{
  		missingField = 'First Name';
  		return false;
  	}
  	else if(isEmpty(form.lastName_bill.value) || (form.lastName_bill.value == "Last Name"))
