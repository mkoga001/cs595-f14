     janrain.events.onCaptureLoginStart.addHandler(function(result){
    	 consoleLog('onCaptureLoginStart');
    	 consoleLog(result);
     });
          
     janrain.events.onCaptureForgotPasswordCodeSuccess.addHandler(function(result){
