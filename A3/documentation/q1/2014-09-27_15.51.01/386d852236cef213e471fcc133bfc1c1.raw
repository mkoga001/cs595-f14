           	 );
    	}
        getTokenForCode(result.authorizationCode, janrain.settings.capture.redirectUri);
    });
    
    janrain.events.onCaptureRegistrationSuccess.addHandler(function(result){
    	consoleLog("onCaptureRegistrationSuccess");
    	consoleLog(result);
    	if (result.action == "traditionalRegister" && enable_push_login_analytics) {
    		callLoginAnalytics(
				'/janrain_email_confirm_' + artistname,
				'Registration success traditional: janrain_email_confirm_' + artistname
			);
    	}
        getTokenForCode(result.authorizationCode, janrain.settings.capture.redirectUri);
    });
    
    janrain.events.onCaptureSaveSuccess.addHandler(function(result){
    	consoleLog("onCaptureSaveSuccess");
