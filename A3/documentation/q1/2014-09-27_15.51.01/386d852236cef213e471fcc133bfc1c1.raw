function janrainCaptureWidgetOnLoad() {
    //check for access token in localStorage and create session
    if(localStorage && localStorage.getItem("janrainCaptureTokenWP")) {
        janrain.capture.ui.createCaptureSession(localStorage.getItem("janrainCaptureTokenWP"));
        localStorage.removeItem("janrainCaptureTokenWP");
    }
    
    // Check to see if the user has successfully reset their password
    if (enable_push_login_analytics) {
    	
    	consoleLog("Check whether password reset cookie set");
    	
    	// If we've got a cookie set, then we should fire a google analytics call
    	if (getCookie('janrain_login_analytics_password_reset') != "") {
    		consoleLog("Paassword reset cookie set");
	    	callLoginAnalytics(
	   			 '/janrain_new_password_confirm_' + artistname,
	   			 'User sucessfully changed their password: janrain_new_password_confirm_' + artistname
	       	 );
	    	
	    	// Now we need to delete the cookie
	    	consoleLog("Paassword reset cookie deleted");
	    	document.cookie = "janrain_login_analytics_password_reset=; expires=Thu, 01 Jan 1970 00:00:00 GMT";
    	} else {
    		consoleLog("Paassword reset cookie NOT set");
    	}
    }
    
    janrain.events.onModalOpen.addHandler(function(result) {
    	consoleLog("onModalOpen");
