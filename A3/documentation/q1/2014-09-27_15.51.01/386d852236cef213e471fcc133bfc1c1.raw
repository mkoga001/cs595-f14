         if (enable_push_login_analytics && !localModalOpen && !emailResetForm) {        	        	 
        	 callLoginAnalytics(
    			 '/janrain_signup_' + artistname,
    			 'Started sign in process: janrain_signup_' + artistname
        	 );
        	 localModalOpen = true;
         }
    });
    
    janrain.events.onModalClose.addHandler(function(result) {
    	consoleLog("onModalClose");
    	consoleLog(result);
         
         if (enable_push_login_analytics) {
        	 callLoginAnalytics(
    			 '/janrain_close_modal_' + artistname,
                 'Modal window closed: janrain_close_modal_' + artistname
        	 );
         }
         
         localModalOpen = false;
         localEmailRegForm = false;
    });
    
    janrain.events.onCaptureRenderComplete.addHandler(function(result) {
    	consoleLog("onCaptureRenderComplete");
    	consoleLog(result);
    	
    	if (result.screen == "forgotPassword" && enable_push_login_analytics) {
    		callLoginAnalytics(
				'/janrain_new_password_' + artistname,
				'Password retrival started: janrain_new_password_' + artistname
    		);
    	}
    	
    });
    
    janrain.events.onCaptureSessionFound.addHandler(function(result){
    	consoleLog("onCaptureSessionFound");
    	consoleLog(result);
    	if(!sessionTracked && enable_push_login_analytics){
    		callLoginAnalyticsEvent('JanRain','Logged In', artistname);
            sessionTracked = true;
        }
    });
    janrain.events.onCaptureSessionNotFound.addHandler(function(result){
    	consoleLog("onCaptureSessionNotFound");
    	consoleLog(result);
