<script type="script/template" id="chorus-modal-wrapper-template" class="template">
  <div class="m-chorus-modal__wrapper m-chorus-modal__root"></div>
</script>
<script type="script/template" id="chorus-modal-window-template" class="template">
  <div class="m-chorus-modal__window">
    [% if (data.title != "" && false) { %]
      <div class="m-chorus-modal__head">
        <div class="m-chorus-modal__title">[%= data.title %]</div>
        <div class="m-chorus-modal__close-button" data-chorus-modal-action="[% if (data.cancel_confirm === true) { %]cancel_confirm[% } else { %]cancel[% } %]">&#215;</div>
      </div>
    [% } %]
    <div class="m-chorus-modal__body m-chorus-modal__window-root"></div>
  </div>
</script>

<script type="script/template" id="explainers-card-template">

  <div class="m-explainer__card-wrapper">
    <div class="m-explainer__pager">
      <a class="m-explainer__pager-control previous" href="[%= data.prevUrl || '#' %]">&lt;</a>
      [% if (data.position && data.total) { %]
        <p class="m-explainer__pager__card-count">Card [%= data.position %] of [%= data.total %]</p>
      [% } %]
      <a class="m-explainer__pager-control next" href="[%= data.nextUrl || '#' %]">&gt;</a>
    </div>

    <div class="m-explainer__card-content">
      [% if (data.title) { %]
        <h2 class="m-explainer__card__headline">[%= data.title %]</h2>
      [% } %]
      [%= data.body || '<div class="m-explainer__card__spinner"></div>' %]
    </div>

    <div class="m-explainer__card__lower">
      <div class="m-explainer__pager lower">
        <a class="m-explainer__pager-control previous" href="[%= data.prevUrl %]">&lt;</a>

      [% if (data.shortUrl && data.shareUrl) { %]
        <div class="m-explainer__card-social">
        Share <span class="this">This</span> Card
          <a class="m-explainer__social facebook" href="https://www.facebook.com/sharer/sharer.php?u=[%= data.shareUrl %]" onclick="window.open(this.href, '', 'height=279,width=575'); return false;">Facebook</a>
          <a class="m-explainer__social twitter" href="https://twitter.com/intent/tweet?url=[%= data.shareUrl %]&text=[%= data.title %]">twitter</a>
          <a class="m-explainer__social google_plus" href="https://plus.google.com/share?url=[%= data.shortUrl %]" onclick="window.open(this.href, '', 'height=279,width=575'); return false;">google plus</a>
          <a class="m-explainer__social print" href="javascript:window.print()">print</a>
        </div>
      [% } %]

        <a class="m-explainer__pager-control next" href="[%= data.nextUrl %]">&gt;</a>
      </div>
    </div>
  </div>
</script>

<script type="script/template" id="explainers-embed-template">
  <div class="m-explainer-embed__header">
    Card [%= data.currentCard %] of [%= data.cardCount %]
    <a href="[%= data.url %]">Launch cards</a>
  </div>

  <div class="m-explainer-embed__main">
    <h4><a href="[%= data.url %]">[%= data.cardTitle %]</a></h4>
    <div class="m-explainer-embed__cardbody">[%= data.cardBody %]</div>
  </div>

  <div class="m-explainer-embed__footer">
    <a href="[%= data.url %]">[%= data.title %]</a>
    [%= data.cardCount %] Cards / <span class="author">Edited By [%= data.userName %]</span> <span>Updated [%= data.updatedAt %]</span>
  </div>
</script>

<script type="script/template" id="explainers-explore-template">
  <div class="m-explainer__card-wrapper">
    [% if (data.exploreCardMode) { %]
    <div class="m-explainer__pager">
      <a class="m-explainer__pager-control previous" href="[%= data.prevUrl || '#' %]">&lt;</a>
      [% if (data.position && data.total) { %]
        <p class="m-explainer__pager__card-count">Card [%= data.position %] of [%= data.total %]</p>
      [% } %]
      <a class="m-explainer__pager-control next" href="[%= data.nextUrl || '#' %]">&gt;</a>
    </div>
    [% } %]

    <div class="m-explainer__card-content">
      <nav class="m-explainer-explore__nav">
        <input id="__query" class="m-explainer-explore__nav__input" type="text">
      </nav>
      <div class="m-explainer-explore__break" id="cards_browse_copy">
        <div class="m-explainer-explore__break__text">Browse card stacks</div>
      </div>
      <div class="m-explainer-explore__card-wrap">
        <div id="__results"><div class="m-explainer__card__spinner"></div></div>
      </div>
    </div>
  </div>
</script>




<script type="script/template" id="explainers-explore-results-template">
[% if (data.cardStacks && data.cardStacks.length > 0) { %]
  [% data.cardStacks.each(function(cardStack) { %]
    <a class="m-explainer-explore__explainer" href="[%= cardStack.get('url') %]">
      <div class="m-explainer-explore__explainer__stack"></div>
      <div class="m-explainer-explore__explainer__head">
        <div class="m-explainer-explore__explainer__head__count">[%= cardStack.get('cardsCount') %] cards</div>
      </div>
      <div class="m-explainer-explore__explainer__body">
        <h2 class="m-explainer-explore__explainer__title">[%= cardStack.get('title') %]</h2>
        <h3 class="m-explainer-explore__explainer__time">Updated [%= cardStack.get('updatedAt') %]</h3>
      </div>
    </a>
  [% }) %]
[% } else { %]
  <div class="m-explainer-explore__no-results">
    <div class="m-explainer-explore__no-results__sorry">
      <h2>Sorry! There’s no card stack with that term.</h2>
      <p>If that seems impossible, please czech your spelling.</p>
    </div>
    <div class="m-explainer-explore__no-results__submit">
      <p>If you think there should be a card stack for this term, click the submit button to send in your suggestion.</p>
      <button>Submit!</button>
      <p>If you’d like to send a more detailed suggestion, <a href="mailto:editors@vox.com?subject=Card stack submission">contact us here</a>. Thanks!</p>
    </div>
  </div>
  [% if (data.suggestedCardStacks && data.suggestedCardStacks.length > 0) { %]
    <div class="m-explainer-explore__break">
      <div class="m-explainer-explore__break__text">Or try one of these!</div>
    </div>
    [% data.suggestedCardStacks.each(function(cardStack) { %]
      <a class="m-explainer-explore__explainer" href="[%= cardStack.get('url') %]">
        <div class="m-explainer-explore__explainer__stack"></div>
        <div class="m-explainer-explore__explainer__head">
          <div class="m-explainer-explore__explainer__head__count">[%= cardStack.get('cardsCount') %] cards</div>
        </div>
        <div class="m-explainer-explore__explainer__body">
          <h2 class="m-explainer-explore__explainer__title">[%= cardStack.get('title') %]</h2>
          <h3 class="m-explainer-explore__explainer__time">Updated [%= cardStack.get('updatedAt') %]</h3>
        </div>
      </a>
    [% }) %]
  [% } %]
[% } %]
</script>
<script type="script/template" id="explainers-fullscreen-template">
  <div class="m-explainer__top-nav">
    <a href="/" class="m-explainer__top-nav-logo"></a>
    <a class="p-hamburger" id="mobile-rail-toggle" href="#" title="Menu">
      <span class="p-hamburger__lines"></span>
    </a>

    <div class="m-explainer__explainer-share">
      <span>Share Card Stack</span>
      <a class="m-explainer__social facebook" href="https://www.facebook.com/sharer/sharer.php?u=[%= data.url %]" onclick="window.open(this.href, '', 'height=279,width=575'); return false;">facebook</a>
      <a class="m-explainer__social twitter" href="https://twitter.com/intent/tweet?url=[%= data.url %]&text=[%= data.title %]">twitter</a>
      <a class="m-explainer__social google_plus" href="https://plus.google.com/share?url=[%= data.url %]" onclick="window.open(this.href, '', 'height=279,width=575'); return false;">google plus</a>
      <a class="m-explainer__social print" href="javascript:window.print()">print</a>
      <a href="[%= data.exitUrl %]" class="m-explainer__close">Exit</a>
    </div>
  </div>

  <div class="m-explainer__rail-overlay" id="mobile-rail-overlay"></div>

  <div class="m-explainer__rail" id="explainer-rail">
    <div class="m-explainer__rail__header" id="explainer-rail-header">
      <div class="m-explainer__rail__header-contents">
        <h2 class="m-explainer__title">[%= data.title %]</h2>
        <div class="m-explainer__meta">
          <p class="m-explainer__meta__byline">By <a href="[%= data.userUrl %]" class="m-explainer__meta__author">[%= data.userName %]</a></p>
          <p class="m-explainer__meta__timestamp">[%= data.updatedAt %]</p>
        </div>
      </div>
    </div>

    <p class="m-explainer__toc-scroller up"></p>
    <ol class="m-explainer__toc" id="explainer-rail-list">
      [% _.each(data.cards, function(card, i) { %]
        <li><a href="[%= card.url %]" data-index="[%= i %]">[%= card.title %]</a></li>
      [% }) %]
    </ol>
    <p class="m-explainer__toc-scroller down"></p>
  </div>

  [% if (data.photoCredit) { %]
  <div class="m-explainer__photo-credit">
    [% if (data.photoCreditUrl) { %]
      <a target="_blank" href="[%= data.photoCreditUrl %]">[%= data.photoCredit %]</a>
    [% } else { %]
      [%= data.photoCredit %]
    [% } %]
  </div>
  [% } %]

  <ol id="card-list" class="m-explainer__cards"></ol>
</script>



