<script>
YUI().use("node", "io", "dump", "json-parse", function (Y) {
    // capture the node that we'll display the messages in
    var jsonTarget = Y.one('#infinite-load span'),
        loadMore = Y.one('#infinite-load'),
        indexCount =  jsonTarget.getAttribute('json-value');

        if(indexCount == 0){
          var targetMain = Y.one('#latest-news-list-0');
        }else{
          var targetMain = Y.one('#latest-news-list-'+indexCount);
        }
        // Create the io callback/configuration
    var callback = {

        timeout : 3000,

        on : {
            success : function (x,o) {
                //Y.log("RAW JSON DATA: " + o.responseText);

                var nextData = Y.JSON.parse(o.responseText),
                    _perBatch = '10',
                    _idxCount =  jsonTarget.getAttribute('json-value'),
                    _nextDataLength = nextData.length * _idxCount, 
                    _batchValue = _perBatch * _idxCount;

                if(_nextDataLength < _batchValue){
                    //Hide load more button
                    loadMore.setStyle('visibility','hidden');
    
                }    

                if (o.responseText){
                    setTimeout(function () {
                        loadMore.setStyle('visibility','hidden');
                    },1000);
                } 

                var messages = [],
                    html = '', i, l;

                // Process the JSON data returned from the server
                try {
                    messages = Y.JSON.parse(o.responseText);
                }
                catch (e) {
                    //alert("JSON Parse failed!");
                    return;
                }
                //Y.log("PARSED DATA: " + Y.Lang.dump(messages));

                for (i=0, l=messages.length; i < l; ++i) {
                    var chronicleId =           messages[i].chronicleId,
                        articleUri =            messages[i].articleUri,
                        relatedImageUri =       messages[i].relatedImageUri,
                        storyHeadline =         messages[i].storyHeadline,
                        newsBoxHeadline =       messages[i].newsBoxHeadline,
                        caption =               messages[i].caption,
                        contentFlag =           messages[i].contentFlag,
                        isCommentable =         messages[i].isCommentable,
                        originalPublishedDate = messages[i].originalPublishedDate,
                        originalPublishedTime = messages[i].originalPublishedTime,
                        modifyDate =            messages[i].modifyDate,
                        modifyTime =            messages[i].modifyTime,
                        writerName =            messages[i].writerName,
                        writerFirstName =       messages[i].writerFirstName,
                        writerLastName =        messages[i].writerLastName;    

                    var query = window.location.search.substring(1),
                        queryId =  query.slice(3,19);
                            
                    if(queryId == chronicleId){
                        var hide = 'hidden';
                    }else{
                        var hide = 'visible';
                    }               

                    if(relatedImageUri != null || relatedImageUri != undefined){
                        var noImage = '';
                    }else{
                        var noImage = 'noImage';                        
                    }     
                    
                    if(contentFlag.toLowerCase() == 'fotw'){
                        var fotw = 'fotw';  
                    }else{
                        var fotw = '';                      
                    }   

                    if(!isCommentable){
                        var hascomment = 'hidden';  
                    }else{
                        var hascomment = '';                      
                    } 
                  html += '<li class="show '+hide+'">'+
                    '<div class="news-stream-module news-stream-writer-archive cf" data-id="' + chronicleId + '">'+                     
                      '<div class="image col '+noImage+'">'+ 
                        '<a href="'+ articleUri +'" class="fresh-wire '+ fotw +'">FRESH OFF</a>'+
                        '<a href="'+ articleUri +'" class="fresh-wire-bottom '+ fotw +'">THE WIRE</a>'+
                        '<div class="image-container">'+
                          '<a href="'+ articleUri +'" class="image-wrapper">'+
                          '<img id="news-page-'+ chronicleId + '-img" alt=" " data-image="'+nfl.constants.ECM_IMAGE_PATH+relatedImageUri+ '" src="'+nfl.constants.ECM_IMAGE_PATH+ relatedImageUri + '" >'+
                          '</a>'+
                        '</div>'+
                      '</div>'+
                      '<div class="text col '+noImage+' caption">'+
                        '<h3><a href="'+ articleUri +'" class="heading">'+ newsBoxHeadline +'</a></h3>'+
                        '<div class="published-date">Published: '+originalPublishedDate+' at '+originalPublishedTime+' </div>'+
                        '<p>'+caption+' <a href="'+ articleUri +'" class="more"> Read</a>'+
                        '</p>'+
                        '<div class="comments '+hascomment+'" style="background-image: url(http://i.nflcdn.com/static/site/6.3/img/home-2011/comment-count.png)"><a class="comment-count" href="'+ articleUri +'#news-article-comments" data-fburl="/goto?id='+ chronicleId + '" data-pluckid="' + chronicleId + '"><fb:comments-count href="http://nfl.com/goto?id='+ chronicleId + '"></fb:comments-count></a></div>'+
                      '</div>'+
                    '</div>'+
                  '</li>';
                }
                targetMain.append(html);
                nfl.loadFacebook(function (FB) {
                    FB.XFBML.parse();
                });
                     
            },

            failure : function (x,o) {
                jsonTarget.hide();
                loadMore.setHTML('<p>Cannot load data. Please try again later.</p>');
                console.log("Check Data if available");
            },

            end: function (x,o) {
                Y.fire('isDoneLoading', true);
            }    
        }
    };

    function setAttr(){
        indexCount++;
        var setCount =  jsonTarget.setAttribute('json-value', indexCount);
    } 
    function loadJson(){
     
        var jsonValue = '/ajax/news?partnerId=around-the-league&maxResults=10&batchNum='+indexCount+'';
        //console.log("jsonValue:"+jsonValue);
        Y.io(jsonValue, callback);
    }
    function loader(){
        loadMore.setStyle('visibility','visible');
    }

    //Initialize 
    Y.on('domready', function () {
        loadJson();
    },'#latest-news-list-1');

    //Infinite Scroll    
    Y.on('scroll',Y.bind(function(_event){
        var footerId = '#footer-2012';

        if (Y.one('#ft') !== null) {
            footerId = '#ft';
        }
        Y.all(footerId).each(function (item, index) {

            var deviceWidth = Y.one("body").get("winWidth");
            
            //Check if its in Portrait or Landscape mode
            if(deviceWidth > 1024){
                var _viewBotMore = 0;
            }else{
                var _viewBotMore = 50;
            }  

            var _viewTop = Math.round(Y.one(footerId).get('region').bottom),
                _viewBot = item.get('viewportRegion').bottom,
                _rangeEnd = ((_viewTop) <= ( _viewBot + _viewBotMore));
            
            // Wait for event 
            var _isDoneLoading = Y.on('isDoneLoading');

            if(_rangeEnd && _isDoneLoading ) {
                setAttr();
                loader();
                loadJson();                                 
            }
        });
    },this));  
   
});
</script> 
